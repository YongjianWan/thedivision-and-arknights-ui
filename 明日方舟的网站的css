        const n = this._hasEvents ? "," : "";
        this.stream.push(n + t),
        this._hasEvents = !0
    }
    finish() {
        this.stream.push("]", !0);
        const t = function(t) {
            let n = 0;
            for (let r = 0, e = t.length; r < e; r++)
